{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Node (index.js)",
      "type": "pwa-node",
      "request": "launch",
      "program": "${workspaceFolder}/src/index.js",
      "cwd": "${workspaceFolder}",
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}